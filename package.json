  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jimmynguyen/coffeescipt-snake.git"
  },
  "scripts": {
    "build": "coffee -c Gruntfile.coffee && grunt",
    "start": "open index.html",
    "zip": "mkdir coffeescript-snake;mkdir coffeescript-snake/out;ditto index.html coffeescript-snake/;ditto out/app.min.* coffeescript-snake/out;ditto fonts coffeescript-snake/fonts;rm -rf coffeescript-snake.zip;zip -rX coffeescript-snake.zip coffeescript-snake;rm -rf coffeescript-snake"
  },
  "dependencies": {
    "angular": "^1.6.0",
    "bootstrap": "^3.3.7",
    "jquery": "^3.1.1"
  },
  "devDependencies": {
    "coffee-script": "^1.12.2",
    "grunt": "~0.4.1",
    "grunt-coffee-jshint": "^1.0.0",
    "grunt-concat-css": "^0.3.1",
    "grunt-contrib-coffee": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-cssmin": "^1.0.2",
    "grunt-contrib-uglify": "^2.0.0"
  }
